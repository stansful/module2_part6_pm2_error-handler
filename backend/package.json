{
  "name": "module2_part2_node",
  "version": "1.0.0",
  "main": "index.js",
  "author": "stansful <stansful@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run copy:env && npm run pm2:start && npm run pm2:logs",
    "stop": "npm run pm2:stop && npm run pm2:delete",
    "pm2:start": "pm2 start dist/index.js --name gallery --listen-timeout 100000",
    "pm2:stop": "pm2 stop gallery",
    "pm2:delete": "pm2 delete gallery",
    "pm2:logs": "pm2 logs --json",
    "pm2:monit": "pm2 monit",
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write src/**/*",
    "lint": "eslint src --ext .ts --fix",
    "copy:env": "cp .env dist/",
    "copy:static": "cd .. && npm run build:frontend && npm run copy"
  },
  "dependencies": {
    "bcrypt": "5.0.1",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "jsonwebtoken": "8.5.1",
    "mongoose": "6.2.7",
    "multer": "1.4.4",
    "passport": "0.5.2",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pm2": "^5.2.0",
    "ts-exif-parser": "0.2.2"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.13",
    "@types/jsonwebtoken": "8.5.8",
    "@types/multer": "1.4.7",
    "@types/node": "17.0.21",
    "@types/passport": "1.0.7",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "eslint": "8.10.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.5.1",
    "ts-node-dev": "1.1.8",
    "typescript": "4.6.2"
  }
}
